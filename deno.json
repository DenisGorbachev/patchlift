{
  "name": "@dengorbachev/patchlift",
  "version": "0.1.2",
  "license": "MIT",
  "exports": "./index.ts",
  "fmt": {
    "lineWidth": 100500,
    "semiColons": false
  },
  "lint": {
    "rules": {
      "exclude": [
        "no-slow-types"
      ]
    }
  },
  "compilerOptions": {
